        "locale",
        "africaine",
        "internationale",
        "restauration_rapide",
        "traditionnelle"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "heuresOuverture": {
      "type": "string"
    },
    "telephoneContact": {
      "type": "string"
    },
